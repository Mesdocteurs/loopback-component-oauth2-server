{
  "name": "OAuthClientApplication",
  "base": "Application",
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "generated": false,
      "length": 128
    },
    "clientType": {
      "type": "string",
      "enum": ["public", "confidential"]
    },
    "redirectURIs": [ "string" ],
    "tokenEndpointAuthMethod": {
      "type": "string",
      "enum": ["none", "client_secret_post", "client_secret_basic"]
    },
    "grantTypes": [
      {
        "type": "string",
        "enum": ["authorization_code", "implicit", "client_credentials",
          "password", "urn:ietf:params:oauth:grant-type:jwt-bearer",
          "urn:ietf:params:oauth:grant-type:saml2-bearer"]
      }
    ],
    "responseTypes": [
      {
        "type": "string",
        "enum": ["code", "token"]
      }
    ],
    "tokenType": {
      "type": "string",
      "enum": ["bearer", "jwt", "mac"]
    },
    "clientSecret": "string",
    "clientName": "string",
    "clientURI": "string",
    "logoURI": "string",
    "logoB64": "string",
    "scopes": ["string"],
    "contacts": ["string"],
    "tosURI": "string",
    "policyURI": "string",
    "jwksURI": "string",
    "jwks": {
      "type": "object",
      "required": false,
      "default": "null",
      "mysql": {
        "dataLength": 4096,
        "dataType": "TEXT"
      }
    },
    "softwareId": "string",
    "softwareVersion": "string",
    "callbackUrls": null,
    "permissions": null,
    "authenticationEnabled": null,
    "anonymousAllowed": null,
    "authenticationSchemes": null,
    "icon": null,
    "url": null,
    "accessTokenTTL": {
      "type": "number",
      "default": 1209600
    },
    "validateByOutsider": {
      "type": "boolean",
      "default": false
    },
    "partnerId": {
      "type": "number",
      "default": null
    },
    "partnerConfig": {
      "type": "object",
      "required": false,
      "default": "null"
    },
    "roles": ["string"]
  },
  "relations": {
    "appUser": {
      "type": "belongsTo",
      "model": "AppUser",
      "foreignKey": "owner"
    }
  },
  "comments": "https://tools.ietf.org/html/draft-ietf-oauth-dyn-reg-24"
}
